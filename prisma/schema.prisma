generator client {
  provider = "prisma-client"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model Patient {
  id        String   @id @default(uuid()) @db.Uuid
  name      String
  email     String?
  phone     String
  birthDate DateTime
  gender    String
  status    String   @default("ativo")
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
